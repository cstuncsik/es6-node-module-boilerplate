{
  "name": "es6-node-module-boilerplate",
  "version": "0.0.0-semantic-release",
  "description": "ES6 node module boilerplate",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/cstuncsik/es6-node-module-boilerplate.git"
  },
  "author": {
    "name": "Csaba Tuncsik",
    "email": "csaba.tuncsik@gmail.com"
  },
  "scripts": {
    "commit": "git-cz",
    "compile": "babel src -d lib",
    "prepublish": "npm run compile",
    "start": "mocha --watch --compilers js:babel-register",
    "test": "mocha --compilers js:babel-register",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "main": "lib/index.js",
  "keywords": [
    "es6",
    "es2015",
    "node",
    "module",
    "boilerplate"
  ],
  "engines": {
    "node": ">=5"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-preset-es2015": "^6.13.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "commitizen": "^2.8.2",
    "cz-conventional-changelog": "^1.1.6",
    "mocha": "^3.0.0",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
